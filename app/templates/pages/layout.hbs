        <section class="grid-12 outline">
            {{#each debugGrid}}
                <div>{{this}}</div>
            {{/each}}
        </section>

        <section class="grid-12 shrink outline">
            {{#each debugGrid}}
                <div>{{this}}</div>
            {{/each}}
        </section>

