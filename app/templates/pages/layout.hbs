{{#> app app-partial-block=true title=title}}
    {{> header}}
    {{#> main main-partial-block=true}}
        <section class="grid-12 outline">
            {{#each debugGrid}}
                <div>{{this}}</div>
            {{/each}}
        </section>

        <section class="grid-12 shrink outline">
            {{#each debugGrid}}
                <div>{{this}}</div>
            {{/each}}
        </section>

        <section class="grid-11 shrink outline">
            {{#each debugGrid}}
                <div>{{this}}</div>
            {{/each}}
        </section>

        <section class="row outline">
            <div class="col-b10-3 col-b9-5 col-b8-6">1</div>
            <div>2</div>
            <div>3</div>
            <div class="col-3 col-b12-0 col-b5-6">4</div>
            <div>5</div>
            <div class="span-2">6</div>
            <div>7</div>
            <div>8</div>
            <div>9</div>
            <div>10</div>
            <div>11</div>
            <div>12</div>
        </section>

        <div class="list col-3 outline">
            <div>1</div>
            <span>2</span>
            <span>3</span>
            <span>2<br/>5</span>
            <div>5</div>
        </div>

        <div class="col outline">
            <div>1</div>
            <span>2</span>
            <span>3</span>
            <span>2<br/>5</span>
            <div>5</div>
        </div>

        <section class="bar outline">
            <div>1</div>
            <div>2</div>
            <div>3</div>
        </section>

    {{/main}}
    {{> footer}}
{{/app}}